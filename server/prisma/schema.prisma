generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Products {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  image String
  title String
  price Int
}

model Blogs {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  image       String
  title       String
  description String
  date        DateTime
}
